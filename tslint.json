{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ true ],
        "no-console": {
            "severity": "warning",
            "options": [ "debug", "info", "log", "time", "timeEnd", "trace"]
        },
        "no-magic-numbers": true
    },
    "rulesDirectory": []
}