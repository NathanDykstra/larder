{
  "name": "larder",
  "version": "1.0.0",
  "description": "An API for managing household goods in your pantry and beyond.",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node src/tools/copyAssets",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc copy-assets",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,pug --exec npm run dev:start",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "initdb": "ts-node ./tools/initdb"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NathanDykstra/larder.git"
  },
