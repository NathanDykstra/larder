  },
  "homepage": "https://github.com/NathanDykstra/larder#readme",
  "dependencies": {
    "@types/cli-progress": "^3.8.0",
    "@types/filesystem": "0.0.29",
    "@types/mssql": "^6.0.4",
    "@types/mysql": "^2.15.15",
    "@types/pug": "^2.0.4",
    "@types/sqlstring": "^2.2.1",
    "@types/winston-syslog": "^2.0.3",
    "cli-progress": "^3.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
