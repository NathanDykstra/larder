    "@types/express": "^4.17.7",
    "@types/fs-extra": "^9.0.1",
    "@types/node": "^14.0.27",
    "@types/shelljs": "^0.8.8",
    "fs-extra": "^9.0.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
