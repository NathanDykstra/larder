{
  "name": "larder",
  "version": "1.0.0",
  "description": "An API for managing household goods in your pantry and beyond.",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node src/tools/copyAssets",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc copy-assets",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,pug --exec npm run dev:start",
    "start": "node .",
    "test": "mocha -r ts-node/register src/__test__/*.spec.ts --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NathanDykstra/larder.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NathanDykstra/larder/issues"
  },
  "homepage": "https://github.com/NathanDykstra/larder#readme",
  "_moduleAliases": {
    "@models": "dist/models",
    "@data": "dist/data",
    "@util": "dist/util",
    "@routes": "dist/routes"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cli-progress": "^3.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "joi": "^17.2.1",
    "module-alias": "^2.2.2",
    "mssql": "^6.2.1",
    "mysql": "^2.18.1",
    "pug": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "sql.js": "^1.4.0",
    "sqlstring": "^2.3.2",
    "typeorm": "^0.2.28",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.14",
    "@types/chai-http": "^4.2.0",
    "@types/cli-progress": "^3.8.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/filesystem": "0.0.29",
    "@types/fs-extra": "^9.0.1",
    "@types/joi": "^14.3.4",
    "@types/mocha": "^8.0.4",
    "@types/mssql": "^6.0.4",
    "@types/mysql": "^2.15.15",
    "@types/node": "^14.11.7",
    "@types/pug": "^2.0.4",
    "@types/shelljs": "^0.8.8",
    "@types/sqlstring": "^2.2.1",
    "@types/winston": "^2.4.4",
    "@types/winston-syslog": "^2.0.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "fs-extra": "^9.0.1",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  }
}
